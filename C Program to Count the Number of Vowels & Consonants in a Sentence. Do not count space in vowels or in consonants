#include<stdio.h>
#include<string.h>
int main()
{
	int c=0,d=0,e,f,g,i,j,k,z=1,n;
	char a[100],b[100];
	gets(a);
	for(i=0;a[i]!='\0';i++)
	{
		if(a[i]=='a' ||a[i]=='e' ||a[i]=='i' ||a[i]=='o' ||a[i]=='u' || a[i]=='A' ||a[i]=='E' ||a[i]=='I' ||a[i]=='O' ||a[i]=='U') 
		c++;
		else if(a[i]!=' ')
		d++;
	}
	printf("voewls= %d \nconsonants =%d",c,d);
}
