#include <stdio.h>
#include<string.h>
int main()
{
    char s[100],g[100];
    int a,i,j,c=0,d,e=0;
    gets(s);
    a=strlen(s);
    for(i=0;i<a;i++)
    {
        for(j=i+1;j<a;j++)
        {
            if(s[i]==s[j])
            {
            g[e]=s[j];
            s[j]=-1;
            e++;
            }
        }
    }
    for(i=0;i<e;i++)
    printf("%c",g[i]);
}
