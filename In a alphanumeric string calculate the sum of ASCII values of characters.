#include <stdio.h>
#include<string.h>
int main()
{
    char s[100],g[100];
    int a,b=0,c=0,d=0,i,j;
    scanf("%s",s);
    a=strlen(s);
    for(i=0;i<a;i++)
    {
        if(isdigit(s[i]))
        {
        b=s[i]-48;
        s[i]=s[i]-b;
        }
        else
        {
        c=c+s[i];
        }
    }
    printf("%d",c);
}
